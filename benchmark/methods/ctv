#!/bin/bash

name=$1

testfa=../testfa/$name
trainfa=../trainfa/$name

if [ ! -f $testfa ] ; then
	echo Not found testfa=$testfa >> /dev/stderr
	exit 1
fi

if [ ! -f $trainfa ] ; then
	echo Not found trainfa=$trainfa >> /dev/stderr
	exit 1
fi

mkdir -p ../pred/ctv
pred=../pred/ctv/$name

$u64 -cons_tax $testfa -db $trainfa -strand plus -tabbedout $pred \
  -strand plus -id 0.7 -maxaccepts 10 -maxrejects 32 -maj 0.51
